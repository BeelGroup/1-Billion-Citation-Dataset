  <macro name="author">
    <names variable="author" prefix="&lt;author>" suffix="&lt;/author>">
      <name initialize-with="."><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>"/>
        <names variable="translator" prefix="&lt;translator>" suffix="&lt;/translator>"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>">
      <name initialize-with="."><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date date-parts="year" form="text" variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>"/>
      </if>
      <else>
        <text term="no date" form="short" prefix="" suffix=""/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group prefix="(" suffix=")">
      <text variable="publisher" suffix="&lt;/publisher>, " prefix="&lt;publisher>"/>
      <text macro="year-date" prefix="" suffix=""/>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short" prefix="" suffix=""/>
        </group>
      </if>
      <else>
        <text variable="edition" prefix="&lt;edition>" suffix="&lt;/edition>"/>
      </else>
    </choose>
  </macro>
  <macro name="citation-locator">
    <choose>
      <if variable="locator">
        <choose>
          <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
            <group delimiter=" " prefix=", ">
              <text term="at" prefix="" suffix=""/>
              <label variable="page" form="short"/>
              <text variable="locator" prefix="&lt;locator>" suffix="&lt;/locator>"/>
            </group>
          </if>
          <else>
            <group delimiter=" " prefix=", ">
              <text term="at" prefix="" suffix=""/>
              <text variable="locator" prefix="&lt;locator>" suffix="&lt;/locator>"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <citation>
    <layout suffix="." delimiter="; ">
      <choose>
        <if position="ibid">
          <text term="ibid" prefix="" suffix=""/>
        </if>
        <else>
          <text macro="author" suffix=", " prefix=""/>
          <choose>
            <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
              <group delimiter=" ">
                <text variable="title" font-style="italic" prefix="&lt;title>" suffix="&lt;/title>"/>
                <text macro="edition" prefix="" suffix=""/>
                <text macro="publisher" prefix="" suffix=""/>
              </group>
            </if>
            <else-if type="chapter paper-conference" match="any">
              <text variable="title" quotes="true" suffix="&lt;/title>, " prefix="&lt;title>"/>
              <text term="in" suffix=" " prefix=""/>
              <text macro="editor" suffix=", " prefix=""/>
              <text variable="container-title" form="short" text-case="title" font-style="italic" suffix="&lt;/container-title>, " prefix="&lt;container-title>"/>
              <text macro="edition" suffix=", " prefix=""/>
              <text macro="publisher" suffix=", " prefix=""/>
              <group delimiter=" ">
                <label variable="page" form="short"/>
                <text variable="page" prefix="&lt;page>" suffix="&lt;/page>"/>
              </group>
            </else-if>
            <else-if type="patent">
              <group delimiter=", ">
                <text variable="title" prefix="&lt;title>" suffix="&lt;/title>"/>
                <text variable="number" prefix="&lt;number>" suffix="&lt;/number>"/>
                <text macro="year-date" prefix="" suffix=""/>
              </group>
            </else-if>
            <else-if type="thesis">
              <group delimiter=", ">
                <text variable="title" quotes="true" prefix="&lt;title>" suffix="&lt;/title>"/>
                <group delimiter=", " prefix="(" suffix=")">
                  <text variable="genre" prefix="&lt;genre>" suffix="&lt;/genre>"/>
                  <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
                  <date date-parts="year-month" form="text" variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>"/>
                </group>
                <group delimiter=": ">
                  <text term="available at" prefix="" suffix=""/>
                  <text variable="URL" prefix="&lt;URL>" suffix="&lt;/URL>"/>
                </group>
              </group>
            </else-if>
            <else-if type="webpage post-weblog" match="any">
              <group delimiter=": ">
                <text term="available at" text-case="capitalize-first" prefix="" suffix=""/>
                <text variable="URL" prefix="&lt;URL>" suffix="&lt;/URL>"/>
              </group>
            </else-if>
            <else>
              <group delimiter=" ">
                <text variable="title" quotes="true" prefix="&lt;title>" suffix="&lt;/title>"/>
                <text macro="year-date" prefix="(" suffix=")"/>
                <group>
                  <text variable="volume" prefix="&lt;volume>" suffix="&lt;/volume>"/>
                  <text variable="issue" prefix="(&lt;issue>" suffix="&lt;/issue>)"/>
                </group>
                <text variable="container-title" text-case="title" font-style="italic" suffix="&lt;/container-title>," prefix="&lt;container-title>"/>
                <group delimiter=" ">
                  <label variable="page" form="short"/>
                  <text variable="page" form="short" prefix="&lt;page>" suffix="&lt;/page>"/>
                </group>
              </group>
            </else>
          </choose>
        </else>
      </choose>
      <text macro="citation-locator" prefix="" suffix=""/>
    </layout>
  </citation>
</style>