  <macro name="author">
    <names variable="author" prefix="&lt;author>" suffix="&lt;/author>">
      <name initialize-with=". " delimiter=" – " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-sort">
    <names variable="author" prefix="&lt;author>" suffix="&lt;/author>">
      <name name-as-sort-order="all" initialize-with=". " delimiter=" – " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>">
      <name initialize-with=". " delimiter=" – " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="editor-sort">
    <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>">
      <name name-as-sort-order="all" initialize-with=". " delimiter=" – " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="translator">
    <names variable="translator" prefix="&lt;translator>" suffix="&lt;/translator>">
      <name initialize-with=". " delimiter=" — " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="translator-sort">
    <names variable="translator" prefix="&lt;translator>" suffix="&lt;/translator>">
      <name name-as-sort-order="all" initialize-with=". " delimiter=" — " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" prefix=" (" suffix=")"/>
    </names>
  </macro>
  <macro name="title">
    <text variable="title" font-style="italic" prefix="&lt;title>" suffix="&lt;/title>"/>
  </macro>
  <macro name="title-short">
    <text variable="title" form="short" font-style="italic" prefix="&lt;title>" suffix="&lt;/title>"/>
  </macro>
  <macro name="series-title">
    <text variable="collection-title" font-style="italic" prefix="(&lt;collection-title>" suffix="&lt;/collection-title>)"/>
  </macro>
  <macro name="editor-container-title">
    <text term="in" suffix=" " prefix=""/>
    <choose>
      <if variable="container-author">
        <names variable="container-author" suffix="&lt;/container-author>, " prefix="&lt;container-author>">
          <name initialize-with=". " delimiter=" – " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
        </names>
      </if>
      <else-if variable="collection-editor">
        <names variable="collection-editor" prefix="&lt;collection-editor>" suffix="&lt;/collection-editor>">
          <name initialize-with=". " delimiter=" — " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
          <label form="short" prefix=" (" suffix="), "/>
        </names>
      </else-if>
    </choose>
    <choose>
      <if variable="editor">
        <names variable="editor" prefix="&lt;editor>" suffix="&lt;/editor>">
          <name initialize-with=". " delimiter=" – " delimiter-precedes-last="always" font-variant="small-caps"><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
          <label form="short" prefix=" (" suffix="), "/>
        </names>
      </if>
    </choose>
    <text variable="container-title" font-style="italic" prefix="&lt;container-title>" suffix="&lt;/container-title>"/>
  </macro>
  <macro name="date">
    <date variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>">
      <date-part name="day" form="numeric" suffix="&lt;/day>/" range-delimiter="-" prefix="&lt;day>"/>
      <date-part name="month" form="numeric" suffix="&lt;/month>/" prefix="&lt;month>"/>
      <date-part name="year" range-delimiter="–" prefix="&lt;year>" suffix="&lt;/year>"/>
    </date>
  </macro>
  <macro name="accessed">
    <date variable="accessed" prefix="&lt;accessed>" suffix="&lt;/accessed>">
      <date-part name="day" suffix="&lt;/day>/" range-delimiter="-" prefix="&lt;day>"/>
      <date-part name="month" form="numeric" suffix="&lt;/month>/" prefix="&lt;month>"/>
      <date-part name="year" prefix="&lt;year>" suffix="&lt;/year>"/>
    </date>
  </macro>
  <macro name="publisher">
    <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
  </macro>
  <macro name="publisher-place-year">
    <text variable="publisher-place" suffix="&lt;/publisher-place> " prefix="&lt;publisher-place>"/>
    <text macro="date" prefix="" suffix=""/>
    <number variable="edition" vertical-align="sup"/>
  </macro>
  <macro name="journal">
    <choose>
      <if variable="container-title">
        <text variable="container-title" prefix="«&lt;container-title>" suffix="&lt;/container-title>»"/>
      </if>
      <else>
        <text variable="container-title" form="short" prefix="«&lt;container-title>" suffix="&lt;/container-title>»"/>
      </else>
    </choose>
  </macro>
  <macro name="volref">
    <choose>
      <if variable="volume number-of-volumes" match="all">
        <group delimiter=" ">
          <text term="volume" form="short" plural="false" prefix="" suffix=""/>
          <text variable="volume" prefix="&lt;volume>" suffix="&lt;/volume>"/>
          <text variable="number-of-volumes" prefix=" (&lt;number-of-volumes>" suffix="&lt;/number-of-volumes>"/>
          <text term="volume" form="short" plural="true" prefix=" " suffix=")"/>
        </group>
      </if>
      <else>
        <group delimiter=" ">
          <text term="volume" form="short" plural="false" prefix="" suffix=""/>
          <text variable="volume" prefix="&lt;volume>" suffix="&lt;/volume>"/>
        </group>
        <group delimiter=" ">
          <text variable="number-of-volumes" prefix="&lt;number-of-volumes>" suffix="&lt;/number-of-volumes>"/>
          <text term="volume" form="short" plural="true" prefix="" suffix=""/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="pageref">
    <label variable="locator" form="short" suffix=" " prefix=", "/>
    <text variable="locator" prefix="&lt;locator>" suffix="&lt;/locator>"/>
  </macro>
  <macro name="pages">
    <text term="page" form="short" plural="true" suffix=" " prefix=", "/>
    <text variable="page" prefix="&lt;page>" suffix="&lt;/page>"/>
  </macro>
  <macro name="urlref">
    <text variable="URL" prefix=" [&lt;URL>" suffix="&lt;/URL>]"/>
    <group prefix=", ">
      <text term="accessed" form="long" suffix=" " prefix=""/>
      <text macro="accessed" prefix="" suffix=""/>
    </group>
  </macro>
  <citation>
    <layout prefix="" suffix="" delimiter="; ">
      <choose>
        <if position="ibid-with-locator">
          <text term="ibid" text-case="capitalize-first" font-style="italic" suffix="." prefix=""/>
          <text macro="pageref" prefix="" suffix=""/>
        </if>
        <else-if position="ibid">
          <text term="ibid" text-case="capitalize-first" font-style="italic" suffix="." prefix=""/>
        </else-if>
        <else-if position="subsequent">
          <group delimiter=", ">
            <text macro="author" prefix="" suffix=""/>
            <text macro="title-short" prefix="" suffix=""/>
            <text term="cited" form="short" prefix="" suffix=""/>
          </group>
          <text macro="pageref" prefix="" suffix=""/>
        </else-if>
        <else-if type="book">
          <group delimiter=", ">
            <text macro="author" prefix="" suffix=""/>
            <text macro="editor" prefix="" suffix=""/>
            <text macro="title" prefix="" suffix=""/>
            <text macro="translator" prefix="" suffix=""/>
            <text macro="volref" prefix="" suffix=""/>
            <text macro="publisher" prefix="" suffix=""/>
            <text macro="publisher-place-year" prefix="" suffix=""/>
          </group>
          <text macro="pageref" prefix="" suffix=""/>
        </else-if>
        <else-if type="article-journal">
          <group delimiter=", ">
            <text macro="author" prefix="" suffix=""/>
            <text macro="title" prefix="" suffix=""/>
            <text macro="journal" prefix="" suffix=""/>
          </group>
          <text variable="volume" prefix=", &lt;volume>" suffix="&lt;/volume>"/>
          <text variable="issue" prefix="/&lt;issue>" suffix="&lt;/issue>"/>
          <text macro="date" prefix=" (" suffix=")"/>
          <choose>
            <if variable="locator">
              <text macro="pageref" prefix="" suffix=""/>
            </if>
            <else-if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </else-if>
            <else-if variable="URL">
              <text macro="urlref" prefix="" suffix=""/>
            </else-if>
          </choose>
        </else-if>
        <else-if type="article-newspaper article-magazine" match="any">
          <group delimiter=", " suffix=", ">
            <text macro="author" prefix="" suffix=""/>
            <text macro="title" prefix="" suffix=""/>
          </group>
          <text term="presented at" suffix=" " prefix=""/>
          <text variable="container-title" font-style="italic" suffix="&lt;/container-title>, " prefix="&lt;container-title>"/>
          <group delimiter=" " suffix=", ">
            <text term="section" prefix="" suffix=""/>
            <text variable="section" font-style="italic" prefix="&lt;section>" suffix="&lt;/section>"/>
          </group>
          <choose>
            <if type="article-magazine">
              <text variable="issue" suffix="&lt;/issue>/" prefix="&lt;issue>"/>
              <text macro="date" prefix="" suffix=""/>
            </if>
            <else>
              <text macro="date" prefix="" suffix=""/>
            </else>
          </choose>
          <choose>
            <if variable="locator">
              <text macro="pageref" prefix="" suffix=""/>
            </if>
            <else-if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </else-if>
            <else-if variable="URL">
              <text macro="urlref" prefix="" suffix=""/>
            </else-if>
          </choose>
        </else-if>
        <else-if type="chapter">
          <group delimiter=", " suffix=", ">
            <text macro="author" prefix="" suffix=""/>
            <text macro="title" prefix="" suffix=""/>
            <text macro="translator" prefix="" suffix=""/>
          </group>
          <group delimiter=" " suffix=", ">
            <text macro="editor-container-title" prefix="" suffix=""/>
            <text macro="series-title" prefix="" suffix=""/>
          </group>
          <group delimiter=", ">
            <text macro="volref" prefix="" suffix=""/>
            <text macro="publisher" prefix="" suffix=""/>
            <text macro="publisher-place-year" prefix="" suffix=""/>
          </group>
          <choose>
            <if variable="locator">
              <text macro="pageref" prefix="" suffix=""/>
            </if>
            <else-if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </else-if>
            <else-if variable="URL">
              <text macro="urlref" prefix="" suffix=""/>
            </else-if>
          </choose>
        </else-if>
        <else-if type="thesis">
          <group delimiter=", ">
            <text macro="author" prefix="" suffix=""/>
            <text macro="title" prefix="" suffix=""/>
          </group>
          <text variable="genre" prefix=", &lt;genre>" suffix="&lt;/genre>, "/>
          <text variable="publisher" suffix="&lt;/publisher>, " prefix="&lt;publisher>"/>
          <text macro="publisher-place-year" prefix="" suffix=""/>
          <choose>
            <if variable="locator">
              <text macro="pageref" prefix="" suffix=""/>
            </if>
            <else-if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </else-if>
            <else-if variable="URL">
              <text macro="urlref" prefix="" suffix=""/>
            </else-if>
          </choose>
        </else-if>
        <else-if type="entry-encyclopedia entry-dictionary" match="any">
          <group delimiter=", ">
            <text macro="author" prefix="" suffix=""/>
            <text macro="title" prefix="" suffix=""/>
            <text macro="editor-container-title" prefix="" suffix=""/>
            <text macro="volref" prefix="" suffix=""/>
          </group>
          <choose>
            <if variable="publisher publisher-place issued" match="all">
              <text variable="publisher" prefix=", &lt;publisher>" suffix="&lt;/publisher>, "/>
              <text macro="publisher-place-year" prefix="" suffix=""/>
            </if>
            <else-if variable="issued">
              <text macro="date" prefix=", " suffix=", "/>
            </else-if>
          </choose>
          <choose>
            <if variable="locator">
              <text macro="pageref" prefix="" suffix=""/>
            </if>
            <else-if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </else-if>
            <else-if variable="URL">
              <text macro="urlref" prefix="" suffix=""/>
            </else-if>
          </choose>
        </else-if>
        <else>
          <group delimiter=", ">
            <text macro="author" prefix="" suffix=""/>
            <text macro="title" prefix="" suffix=""/>
            <text macro="date" prefix=" (" suffix=")"/>
          </group>
          <choose>
            <if variable="locator">
              <text macro="pageref" prefix="" suffix=""/>
            </if>
            <else-if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </else-if>
            <else-if variable="URL">
              <text macro="urlref" prefix="" suffix=""/>
            </else-if>
          </choose>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography hanging-indent="true" subsequent-author-substitute="———" entry-spacing="0">
    
    <layout suffix=".">
      <text macro="author-sort" suffix=", " prefix=""/>
      <choose>
        <if type="book">
          <group delimiter=", ">
            <text macro="editor-sort" prefix="" suffix=""/>
            <text macro="title" prefix="" suffix=""/>
            <text macro="translator-sort" prefix="" suffix=""/>
            <text macro="volref" prefix="" suffix=""/>
            <text macro="publisher" prefix="" suffix=""/>
            <text macro="publisher-place-year" prefix="" suffix=""/>
          </group>
        </if>
        <else-if type="thesis">
          <text macro="title" prefix="" suffix=""/>
          <text variable="genre" prefix=", &lt;genre>" suffix="&lt;/genre>, "/>
          <text variable="publisher" suffix="&lt;/publisher>, " prefix="&lt;publisher>"/>
          <text macro="publisher-place-year" prefix="" suffix=""/>
        </else-if>
        <else-if type="article-journal">
          <group delimiter=", ">
            <text macro="title" prefix="" suffix=""/>
            <text macro="journal" prefix="" suffix=""/>
          </group>
          <text variable="volume" prefix=" &lt;volume>" suffix="&lt;/volume>"/>
          <text variable="issue" prefix="/&lt;issue>" suffix="&lt;/issue>"/>
          <text macro="date" prefix=" (" suffix=")"/>
          <choose>
            <if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </if>
          </choose>
          <text macro="urlref" prefix="" suffix=""/>
        </else-if>
        <else-if type="article-newspaper article-magazine" match="any">
          <text macro="title" suffix=", " prefix=""/>
          <text term="presented at" suffix=" " prefix=""/>
          <text variable="container-title" font-style="italic" suffix="&lt;/container-title>, " prefix="&lt;container-title>"/>
          <group delimiter=" " suffix=", ">
            <text term="section" prefix="" suffix=""/>
            <text variable="section" font-style="italic" prefix="&lt;section>" suffix="&lt;/section>"/>
          </group>
          <choose>
            <if type="article-magazine">
              <text variable="issue" suffix="&lt;/issue>/" prefix="&lt;issue>"/>
              <text macro="date" prefix="" suffix=""/>
            </if>
            <else>
              <text macro="date" prefix="" suffix=""/>
            </else>
          </choose>
          <choose>
            <if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </if>
          </choose>
          <text macro="urlref" prefix="" suffix=""/>
        </else-if>
        <else-if type="chapter">
          <text macro="title" prefix="" suffix=""/>
          <group delimiter=" " prefix=", ">
            <text macro="editor-container-title" prefix="" suffix=""/>
            <text macro="series-title" prefix="" suffix=""/>
          </group>
          <group delimiter=", " prefix=", ">
            <text macro="volref" prefix="" suffix=""/>
            <text macro="publisher" prefix="" suffix=""/>
            <text macro="publisher-place-year" prefix="" suffix=""/>
          </group>
          <choose>
            <if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </if>
          </choose>
          <text macro="urlref" prefix="" suffix=""/>
        </else-if>
        <else-if type="entry-encyclopedia entry-dictionary" match="any">
          <group delimiter=", ">
            <text macro="title" prefix="" suffix=""/>
            <text macro="editor-container-title" prefix="" suffix=""/>
            <text macro="volref" prefix="" suffix=""/>
          </group>
          <choose>
            <if variable="publisher publisher-place issued" match="all">
              <text variable="publisher" prefix=", &lt;publisher>" suffix="&lt;/publisher>, "/>
              <text macro="publisher-place-year" prefix="" suffix=""/>
            </if>
            <else-if variable="issued">
              <text macro="date" prefix=", " suffix=", "/>
            </else-if>
          </choose>
          <choose>
            <if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </if>
          </choose>
          <text macro="urlref" prefix="" suffix=""/>
        </else-if>
        <else>
          <text macro="title" prefix="" suffix=""/>
          <text macro="date" prefix=" (" suffix=")"/>
          <choose>
            <if variable="page">
              <text macro="pages" prefix="" suffix=""/>
            </if>
          </choose>
          <text macro="urlref" prefix="" suffix=""/>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>