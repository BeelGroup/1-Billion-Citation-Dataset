<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" page-range-format="minimal">
  <info>
    <title>Ugeskrift for Læger (Danish)</title>
    <id>http://www.zotero.org/styles/ugeskrift-for-laeger</id>
    <link href="http://www.zotero.org/styles/ugeskrift-for-laeger" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver" rel="template"/>
    <link href="http://ugeskriftet.dk/manuskriptvejledning-0" rel="documentation"/>
    <contributor>
      <name>Svend Gundestrup</name>
      <uri>http://www.mendeley.com/profiles/svend-gundestrup/</uri>
    </contributor>
