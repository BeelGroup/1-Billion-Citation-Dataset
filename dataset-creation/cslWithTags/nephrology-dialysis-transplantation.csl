      <choose>
        <if match="any" is-numeric="page">
          <text variable="page" prefix="; &lt;page>" suffix="&lt;/page>"/>
        </if>
        <else>
          <text value="" prefix="." suffix=""/>
        </else>
      </choose>
      <text macro="accessed-date" prefix=" " suffix=""/>
    </group>
  </macro>
  <macro name="access">
