              <text macro="container-title" prefix="" suffix=""/>
              <text macro="locators" prefix="" suffix=""/>
            </group>
            <text macro="container-contributors-note" prefix="" suffix=""/>
          </group>
          <group delimiter=", " prefix=" (" suffix=")">
            <choose>
              <if type="broadcast motion_picture" match="none">
                <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
              </if>
            </choose>
            <text macro="date-in-parentheses" prefix="" suffix=""/>
          </group>
          <group prefix=", " delimiter=", ">
            <choose>
              <if type="post">
                <text variable="genre" prefix="&lt;genre>" suffix="&lt;/genre>"/>
              </if>
            </choose>
            <text macro="studio" prefix="" suffix=""/>
            <text macro="date-in-text" prefix="" suffix=""/>
            <text macro="description-audio-note" prefix="" suffix=""/>
            <choose>
              <if type="webpage post-weblog" match="any">
                <text variable="URL" prefix="&lt;URL>" suffix="&lt;/URL>"/>
              </if>
            </choose>
            <text macro="point-locators" prefix="" suffix=""/>
          </group>
        </else>
      </choose>
    </layout>
  </citation>
