      <name>Laura Martin</name>
      <email>leros.martin@gmail.com</email>
    </author>
    <contributor>
      <name>Patrick O'Brien</name>
      <email>obrienpat86@gmail.com</email>
    </contributor>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <category field="medicine"/>
    <issn>1865-1674</issn>
