              <label form="verb-short" suffix=" "/>
              <name font-variant="small-caps" delimiter=" / " initialize-with="."><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
