<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" page-range-format="minimal">
  <info>
    <title>Journal of Applied Pharmaceutical Research</title>
    <title-short>JOAPR</title-short>
    <id>http://www.zotero.org/styles/journal-of-applied-pharmaceutical-research</id>
    <link href="http://www.zotero.org/styles/journal-of-applied-pharmaceutical-research" rel="self"/>
    <link href="http://www.zotero.org/styles/vancouver" rel="template"/>
    <link href="http://www.japtronline.com/index.php/JOAPR/about/submissions#authorGuidelines" rel="documentation"/>
    <author>
      <name>BABA Web Design</name>
      <email>babawebdesign@gmail.com</email>
    </author>
    <contributor>
      <name>Sanjib Bahadur</name>
    </contributor>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <eissn>2348-0335</eissn>
    <summary>A Style for Journal of Applied Pharmaceutical Research based on Vancouver style as outlined by International Committee of Medical Journal Editors Uniform Requirements for Manuscripts Submitted to Biomedical Journals: Sample References</summary>
    <updated>2017-03-04T08:17:07+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <style-options punctuation-in-quote="false"/>
  </locale>
  <macro name="author">
    <names variable="author" prefix="&lt;author>" suffix="&lt;/author>">
      <name initialize-with="" name-as-sort-order="all" sort-separator=" "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
    </names>
  </macro>
  <macro name="container-title">
    <choose>
      <if type="article-journal" match="any">
        <text variable="container-title" form="short" font-style="italic" prefix="&lt;container-title>" suffix="&lt;/container-title>"/>
      </if>
      <else-if type="chapter" match="any">
        <text variable="container-title" font-style="italic" prefix="&lt;container-title>" suffix="&lt;/container-title>"/>
      </else-if>
      <else>
        <text variable="container-title" font-style="normal" prefix="&lt;container-title>" suffix="&lt;/container-title>"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <number suffix=" " variable="edition" form="ordinal"/>
    <label plural="never" variable="edition" form="short"/>
  </macro>
  <macro name="event">
    <group delimiter=", ">
      <text variable="event-place" prefix="&lt;event-place>" suffix="&lt;/event-place>"/>
      <group>
        <text value="on" suffix=" " prefix=""/>
        <date variable="event-date" prefix="&lt;event-date>" suffix="&lt;/event-date>">
          <date-part name="day" suffix="&lt;/day> " prefix="&lt;day>"/>
          <date-part name="month" suffix="&lt;/month>, " prefix="&lt;month>"/>
          <date-part name="year" prefix="&lt;year>" suffix="&lt;/year>"/>
        </date>
      </group>
    </group>
  </macro>
  <macro name="title">
    <choose>
      <if type="book" match="any">
        <text variable="title" quotes="false" font-style="italic" prefix="&lt;title>" suffix="&lt;/title>"/>
      </if>
      <else-if type="webpage" match="any">
        <text variable="title" quotes="true" prefix="&lt;title>" suffix="&lt;/title>"/>
      </else-if>
      <else>
        <text variable="title" prefix="&lt;title>" suffix="&lt;/title>"/>
      </else>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if type="article-journal patent" match="any">
        <date date-parts="year" form="numeric" variable="issued" font-weight="normal" prefix="(&lt;issued>" suffix="&lt;/issued>)">
          <date-part name="year" font-weight="normal" prefix="&lt;year>" suffix="&lt;/year>"/>
        </date>
      </if>
      <else-if type="paper-conference" match="any">
        <date variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>">
          <date-part name="day" suffix="&lt;/day> " prefix="&lt;day>"/>
          <date-part name="month" suffix="&lt;/month>, " prefix="&lt;month>"/>
          <date-part name="year" prefix="&lt;year>" suffix="&lt;/year>"/>
        </date>
      </else-if>
      <else-if type="webpage" match="any">
        <date variable="accessed" prefix="&lt;accessed>" suffix="&lt;/accessed>">
          <date-part name="day" suffix="&lt;/day> " prefix="&lt;day>"/>
          <date-part name="month" suffix="&lt;/month>, " prefix="&lt;month>"/>
          <date-part name="year" prefix="&lt;year>" suffix="&lt;/year>"/>
        </date>
      </else-if>
      <else>
        <date date-parts="year" form="numeric" variable="issued" prefix="&lt;issued>" suffix="&lt;/issued>">
          <date-part name="year" prefix="&lt;year>" suffix="&lt;/year>"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="(&lt;editor>" suffix="&lt;/editor>)">
      <name suffix=" " initialize="false" initialize-with="" name-as-sort-order="all" sort-separator=" "><name-part name="family" prefix="&lt;family>" suffix="&lt;/family>"/><name-part name="given" prefix="&lt;given>" suffix="&lt;/given>"/></name>
      <label form="short" plural="never"/>
    </names>
  </macro>
  <macro name="locators">
    <choose>
      <if type="article-journal" match="any">
        <text variable="volume" font-style="normal" font-weight="bold" prefix="&lt;volume>" suffix="&lt;/volume>"/>
      </if>
      <else-if type="book chapter" match="any">
        <group delimiter=", ">
          <group>
            <text term="volume" form="short" text-case="capitalize-first" font-style="normal" suffix=" " prefix=""/>
            <text variable="volume" prefix="&lt;volume>" suffix="&lt;/volume>"/>
          </group>
          <group>
            <text term="chapter" form="short" text-case="capitalize-first" suffix=" " prefix=""/>
            <text variable="chapter-number" prefix="&lt;chapter-number>" suffix="&lt;/chapter-number>"/>
          </group>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="page">
    <choose>
      <if type="book chapter paper-conference" match="any">
        <label variable="page" form="short"/>
        <text variable="page" prefix="&lt;page>" suffix="&lt;/page>"/>
      </if>
      <else>
        <text variable="page" prefix="&lt;page>" suffix="&lt;/page>"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", ">
      <text variable="publisher" prefix="&lt;publisher>" suffix="&lt;/publisher>"/>
      <text variable="publisher-place" prefix="&lt;publisher-place>" suffix="&lt;/publisher-place>"/>
    </group>
  </macro>
  <citation collapse="citation-number">
    
    <layout vertical-align="baseline" delimiter="," prefix="[" suffix="]">
      
    </layout>
  </citation>
  <bibliography entry-spacing="0" second-field-align="flush">
    
    <layout suffix=".">
      
      <text macro="author" suffix=". " prefix=""/>
      <choose>
        <if type="article-journal">
          <group delimiter=". ">
            <text macro="title" prefix="" suffix=""/>
            <group delimiter=", ">
              <text macro="container-title" prefix="" suffix=""/>
              <text macro="locators" prefix="" suffix=""/>
              <group delimiter=" ">
                <text macro="page" prefix="" suffix=""/>
                <text macro="issued" prefix="" suffix=""/>
              </group>
            </group>
          </group>
        </if>
        <else-if type="book" match="any">
          <group delimiter=". ">
            <text macro="title" prefix="" suffix=""/>
            <group delimiter=", ">
              <group delimiter=" ">
                <text macro="edition" prefix="" suffix=""/>
                <text macro="locators" prefix="" suffix=""/>
              </group>
              <text macro="publisher" prefix="" suffix=""/>
              <group delimiter=" ">
                <text macro="issued" prefix="(" suffix=")"/>
              </group>
            </group>
          </group>
        </else-if>
        <else-if type="chapter" match="any">
          <group delimiter=". ">
            <text variable="title" prefix="&lt;title>" suffix="&lt;/title>"/>
            <text macro="container-title" prefix="" suffix=""/>
            <group delimiter=", ">
              <group delimiter=" ">
                <text macro="edition" prefix="" suffix=""/>
                <text macro="editor" prefix="" suffix=""/>
                <text macro="locators" prefix="" suffix=""/>
              </group>
              <text macro="publisher" prefix="" suffix=""/>
              <group delimiter=" ">
                <text macro="page" prefix="" suffix=""/>
                <text macro="issued" prefix="(" suffix=")"/>
              </group>
            </group>
          </group>
        </else-if>
        <else-if type="paper-conference" match="any">
          <group delimiter=", ">
            <text variable="title" prefix="&lt;title>" suffix="&lt;/title>"/>
            <text macro="container-title" prefix="" suffix=""/>
            <text macro="event" prefix="" suffix=""/>
            <text macro="editor" prefix="" suffix=""/>
            <text macro="issued" prefix="" suffix=""/>
            <text macro="publisher" prefix="" suffix=""/>
            <text macro="page" prefix="" suffix=""/>
          </group>
        </else-if>
        <else-if type="patent">
          <group delimiter=" ">
            <text variable="number" prefix="&lt;number>" suffix="&lt;/number>"/>
            <text macro="issued" prefix="" suffix=""/>
          </group>
        </else-if>
        <else-if type="thesis" match="any">
          <group delimiter=", ">
            <text variable="genre" prefix="&lt;genre>" suffix="&lt;/genre>"/>
            <text macro="publisher" prefix="" suffix=""/>
            <text macro="issued" prefix="" suffix=""/>
          </group>
        </else-if>
        <else-if type="webpage" match="any">
          <group delimiter=".">
            <text macro="title" prefix="" suffix=""/>
            <group delimiter=", ">
              <text variable="URL" prefix=": &amp;amp;lt;&lt;URL>" suffix="&lt;/URL>&amp;amp;gt;"/>
              <group delimiter=" ">
                <text term="cited" suffix=" " prefix=""/>
                <text macro="issued" prefix="" suffix=""/>
              </group>
            </group>
          </group>
        </else-if>
        <else>
          <group delimiter=", ">
            <text macro="title" prefix="" suffix=""/>
            <text macro="publisher" prefix="" suffix=""/>
            <text macro="issued" prefix="" suffix=""/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>